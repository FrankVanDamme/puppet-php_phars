{
  "name": "gajdaw-php_phars",
  "version": "0.2.1",
  "author": "gajdaw",
  "summary": "A module to install PHARS: composer.phar, phpunit.phar, etc.",
  "license": "MIT",
  "source": "https://github.com/puppet-by-examples/puppet-php_phars",
  "project_page": "https://github.com/puppet-by-examples/puppet-php_phars",
  "issues_url": "https://github.com/puppet-by-examples/puppet-php_phars/issues",
  "tags": [
    "php",
    "phar",
    "composer",
    "phpunit",
    "php-cs-fixer",
    "box",
    "behat",
    "phpspec",
    "symfony"
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "12.04",
        "14.04"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": "3.7.x"
    }
  ],
  "dependencies": [
    {
      "name": "gajdaw/filefetcher",
      "version_requirement": ">=0.1.2"
    },
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">=4.5.0"
    },
    {
      "name": "gajdaw/diverse_functions",
      "version_requirement": ">=0.1.1"
    }
  ]
}
